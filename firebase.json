{
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "taskotel",
          "appId": "1:972588904282:android:a7db2fada94980b1a84d63",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "taskotel",
          "appId": "1:972588904282:ios:e9d3601b1e7154f0a84d63",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "taskotel",
          "configurations": {
            "android": "1:972588904282:android:a7db2fada94980b1a84d63",
            "ios": "1:972588904282:ios:e9d3601b1e7154f0a84d63",
            "web": "1:972588904282:web:7bd6d60f63197d2fa84d63"
          }
        }
      }
    }
  },
  "firestore": {
    "database": "(default)",
    "location": "asia-south1",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "hosting": {
    "public": "build/web",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
